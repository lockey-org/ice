createdb -U postgres --encoding=UTF8 --locale=C --template=template0 --owner=synapse synapse
dropdb -U postgres synapse
dropuser -U postgres synapse




psql -U synapse -h localhost
update  users SET admin = 1 WHERE name = '@postgres:lkt.icu';



